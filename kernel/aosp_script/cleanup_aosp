echo "Cleaning up old *.zips on your transformer's sdcard root directory and all the outdated stuff in your working directory"
echo
adb shell rm /sdcard/kernel_aosp.zip
echo

cd /home/chris/working_dir/aosp
rm kernelblob.LNX
rm kern.blob
